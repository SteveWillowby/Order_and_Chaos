@article{Ito01022000,
author = {Ito, Takashi and Tashiro, Kosuke and Muta, Shigeru and Ozawa, Ritsuko and Chiba, Tomoko and Nishizawa, Mayumi and Yamamoto, Kiyoshi and Kuhara, Satoru and Sakaki, Yoshiyuki}, 
title = {Toward a protein–protein interaction map of the budding yeast: A comprehensive system to examine two-hybrid interactions in all possible combinations between the yeast proteins},
volume = {97}, 
