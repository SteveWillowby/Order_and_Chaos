@article{Collins01032007,
author = {Collins, Sean R. and Kemmeren, Patrick and Zhao, Xue-Chu and Greenblatt, Jack F. and Spencer, Forrest and Holstege, Frank C. P. and Weissman, Jonathan S. and Krogan, Nevan J.}, 
title = {Toward a Comprehensive Atlas of the Physical Interactome of Saccharomyces cerevisiae},
volume = {6}, 
